<%= provide(:title,"Sign In") %>
<div class="row">
	<div class="span6 offset3 login_form">
		<div class="title"> 
			<span class="text"> Sign In </span>
		</div>
		<%= form_for(:session, url: sessions_path) do |f| %>

			<%= f.label :email %>
			<%= f.text_field :email %>

			<%= f.label :password %>
			<%= f.password_field :password %>

			<%= f.submit "Sign in", class: "btn btn-success" %>  
			 
			<%= link_to "Not yet a user? Sign Up", signup_path , class: "sign_link" %>
			
		<% end %>
	</div>
</div>
