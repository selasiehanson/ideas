<%= provide(:title,"Edit Details") %>
<h1>Edit Details</h1>

<div class="row">
	<div class="span6 offset3">
		<%= form_for(@user)  do|f| %>
			<%= f.label :name %>
			<%= f.text_field :name %>

			<%= f.label :email %>
			<%= f.text_field :email %>

			<%= f.submit "Update Account", class: "btn btn-success"%>
		<% end %>
	</div>
</div>
